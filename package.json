{
	"name": "chambra-app",
	"version": "1.0.0",
	"main": "node_modules/expo/AppEntry.js",
	"scripts": {
		"start": "expo start",
		"android": "expo start --android",
		"ios": "expo start --ios",
		"web": "expo start --web",
		"eject": "expo eject",
		"postinstall": "nvmrc-check --node $(cat .nvmrc)",
		"lint": "tsc --noEmit && eslint . --ext .ts,.tsx,.js,.jsx,.json",
		"lint-and-fix": "eslint . --ext .ts,.tsx,.js,.jsx,.json --fix",
		"prettier-format": "pretty-quick --pattern '**/*.{json,js,jsx,ts,tsx,md}' --verbose",
		"tscheck": "tsc",
		"prepare": "husky install"
	},
	"dependencies": {
		"expo": "~46.0.9",
		"expo-status-bar": "~1.4.0",
		"react": "18.0.0",
		"react-dom": "18.0.0",
		"react-native": "0.69.5",
		"react-native-paper": "^4.12.4",
		"react-native-web": "~0.18.7",
		"styled-components": "^5.3.5"
	},
	"devDependencies": {
		"@babel/core": "^7.12.9",
		"@types/react": "~18.0.14",
		"@types/react-native": "~0.69.1",
		"@types/styled-components": "^5.1.26",
		"@types/styled-components-react-native": "^5.1.3",
		"@typescript-eslint/eslint-plugin": "^5.38.0",
		"@typescript-eslint/parser": "^5.38.0",
		"babel-plugin-module-resolver": "^4.1.0",
		"eslint": "^8.23.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.31.8",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"nvmrc-check": "^1.0.2",
		"prettier": "^2.7.1",
		"pretty-quick": "^3.1.3",
		"typescript": "~4.3.5"
	},
	"private": true,
	"license": "MIT"
}
